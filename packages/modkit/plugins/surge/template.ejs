<%= surgeTemplate.renderMetadata(argumentsText, argumentsDescription) %>


<% if (content.general) { %>
[General]
<%= surgeTemplate.renderKeyValuePairs(content.general) %>
<% } %>

<% if (content.host) { %>
[Host]
<%= surgeTemplate.renderKeyValuePairs(content.host) %>
<% } %>

<% if (content.rule) { %>
[Host]
<%= surgeTemplate.renderRule() %>
<% } %>

<% if (content.script) { %>
[Script]
<%= surgeTemplate.renderScript(scriptParams) %>
<% } %>

<% if (content.mitm && content.mitm.hostname) { %>
[Mitm]
hostname = %APPEND% <%= content.mitm.hostname %>
<% } %>
